{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "RouteScope: Run Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/packages/extension",
        "--disable-extensions"
      ],
      "preLaunchTask": "routescope: build workspaces",
      "outFiles": [
        "${workspaceFolder}/packages/**/dist/**/*.js"
      ]
    }
  ]
}
